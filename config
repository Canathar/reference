Host 10.9.50*
   User mrusse07
   ProxyCommand sshpass -p 'Abcd1234!' ssh -o StrictHostKeyChecking=no sim-nm -W %h:%p
   ServerAliveInterval 120

Host sim-nm
   Hostname 172.24.13.145
   User mrusse07
   ProxyCommand sshpass -p 'natsproxy' ssh -o StrictHostKeyChecking=no proxy@172.24.28.82 -W %h:%p
   ServerAliveInterval 120









